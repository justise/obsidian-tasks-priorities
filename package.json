{
	"name": "tasks-prioritizer",
	"version": "1.0.1",
	"description": "Kanban style prioritization of tasks in Obsidian with Drag and Drop support.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
