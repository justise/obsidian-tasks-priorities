	"devDependencies": {
		"@types/node": "^18.19.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"builtin-modules": "^3.3.0",
		"dotenv": "^17.2.1",
		"esbuild": "^0.25.8",
		"esbuild-plugin-copy": "^2.1.1",
		"eslint": "^9.0.0",
		"obsidian": "latest",
		"tslib": "^2.7.0",
		"typescript": "^5.6.0"
	}
}
